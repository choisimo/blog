{
  "name": "blog-admin",
  "version": "2.0.0",
  "description": "Enhanced blog post management admin panel with auto-deployment to GitHub Pages",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "client": "cd client && npm start",
    "client:build": "cd client && npm run build",
    "dev:full": "concurrently \"npm run dev\" \"npm run client\"",
    "build": "npm run client:build"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "axios": "^1.11.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^4.18.2",
    "fs-extra": "^11.1.1",
    "gray-matter": "^4.0.3",
    "moment": "^2.29.4",
    "multer": "^1.4.5-lts.1",
    "sharp": "^0.34.3",
    "simple-git": "^3.19.1",
    "slugify": "^1.6.6"
  },
  "devDependencies": {
    "concurrently": "^8.2.0",
    "nodemon": "^3.0.1"
  },
  "keywords": [
    "blog",
    "admin",
    "cms",
    "markdown",
    "github-pages",
    "auto-deployment",
    "react",
    "nodejs"
  ],
